<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/c"
    tools:context=".WisataActivity">

    <!--elemen ini digunkan untuk menampilkan teks, teksnya "WISATA INDONESIA"
    layout width dan height menggunakan wrapcontent,
    ukuran jarak marginTopnya 16dp,
    untuk warna teks yang diberikan adalah warna hitam,
    gaya teksnya tebal, ukuran teks 34sp-->
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/wisata_indonesia"
        android:textColor="@color/black"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!--elemen ini digunkan untuk menampilkan teks, teksnya "Jam Gadang, Sumatra Barat"
    layout width dan height menggunakan wrapcontent,
     ukuran jarak marginTopnya 12dp, ukuran margin startnya 84dp,
     ukuran teksnya 12sp
     untuk warna teks yang diberikan adalah warna hitam,
     gaya teksnya tebal-->
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="12dp"
        android:text="@string/jam_gadang_sumatra_barat"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <!--elemen ini digunakan untuk menampilkan gambar,
   ukuran layout width 144dp, layout heigt 101dp,
   ukuran margin start 92dp, marginTopnya 8dp,
   untuk deskripsi gambarnya yaitu Candi borobudur,
   untuk layout contraintStart toStartOfnya menggunakan perent-->
    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="144dp"
        android:layout_height="101dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="8dp"
        android:contentDescription="@string/candi_borobudur"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@drawable/borobudur" />

    <!--elemen ini digunkan untuk menampilkan teks, teksnya "Candi borobudur, Jawa Tengah"
   layout width dan height menggunakan wrapcontent,
   ukuran margintop 8dp, marginstart 84dp,
   ukuran teksnya 12sp,
   warna teks hitam,
   untuk warna teks yang diberikan adalah warna hitam,
   gaya teksnya tebal-->
    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="8dp"
        android:text="@string/candi_borobudur_jawa_tengah"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <!--elemen ini digunakan untuk menampilkan gambar,
    ukuran layout width 151dp, layout heigt 113dp,
    ukuran margin End 14dp, marginTopnya 4dp,
    untuk deskripsi gambarnya yaitu TEBING BREKSI,
    untuk layout contraintStart toStartOfnya menggunakan perent-->
    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="151dp"
        android:layout_height="113dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="140dp"
        android:contentDescription="@string/tebing_breksi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/breksi" />

    <!--elemen ini digunkan untuk menampilkan teks, teksnya "Tebing Breksi, Yogyakarta"
   layout width dan height menggunakan wrapcontent,
   ukuran jarak marginTopnya 4dp, marginEndnya 140dp,
   ukuran teksnya 12sp,
   warna teks hitam,
   untuk warna teks yang diberikan adalah warna hitam,
   gaya teksnya tebal-->
    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="140dp"
        android:text="@string/tebing_breksi_yogyakarta"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <!--elemen ini digunakan untuk menampilkan gambar,
    ukuran layout width 153dp, layout heigt 99dp,
    ukuran margin End 140dp, marginTopnya 8dp,
    untuk deskripsi gambarnya yaitu RAJA AMPAT,
    untuk layout contraintStart toStartOfnya menggunakan perent-->
    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="153dp"
        android:layout_height="99dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="140dp"
        android:contentDescription="@string/raja_ampat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/rajaampat" />

    <!--elemen ini digunkan untuk menampilkan teks, teksnya "Raja Ampat, Papua"
   layout width dan height menggunakan wrapcontent,
   ukuran jarak marginTopnya 4dp, marginEndnya 152dp
   ukuran teksnya 12sp,
   warna teks hitam,
   untuk warna teks yang diberikan adalah warna hitam,
   gaya teksnya tebal-->
    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="152dp"
        android:text="@string/raja_ampat_papua"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <!--elemen ini digunakan untuk menampilkan gambar,
    ukuran layout width 140dp, layout heigt 102dp,
    ukuran marginStartnya 92dp dan marginTop 4dp
    untuk deskripsi gambarnya yaitu JAM GADANG,
    untuk layout contraintStart toStartOfnya menggunakan perent-->
    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="140dp"
        android:layout_height="102dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="4dp"
        android:contentDescription="@string/jam_gadang"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/jamgadang" />

</androidx.constraintlayout.widget.ConstraintLayout>